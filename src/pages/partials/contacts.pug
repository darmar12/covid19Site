.contacts#contacts
    .container.contacts__container
        h3.title.contacts__title
            span Contact 
            |for Assistance
        .contacts__content
            form(action="#").contacts__form
                fieldset.contacts__box
                    legend.contacts__name Name
                    input(type="text", placeholder="", name="name").contacts__input#contactsName
                fieldset.contacts__box
                    legend.contacts__name Email
                    input(type="email", placeholder="", name="email").contacts__input#contactsEmail
                fieldset.contacts__box
                    legend.contacts__name Phone
                    input(type="text", placeholder="", name="phone").contacts__input#contactsPhone
                fieldset.contacts__box
                    legend.contacts__name Message
                    textarea(type="text", placeholder="", name="message").contacts__textarea#contactsMessage
                button.btn.contacts__btn Submit
            .contacts__map
                iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d30153.788252261566!2d72.82321484621745!3d19.141690214227783!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3be7b63aceef0c69%3A0x2aa80cf2287dfa3b!2sJogeshwari%20West%2C%20Mumbai%2C%20Maharashtra%20400047!5e0!3m2!1sen!2sin!4v1627060527760!5m2!1sen!2sin", allowfullscreen, loading="lazy")